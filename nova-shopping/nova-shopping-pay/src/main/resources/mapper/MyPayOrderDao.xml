            </if>
        </trim>
    </insert>

    <update id="updateMyPayOrder" parameterType="com.mall.pay.entity.MyPayOrder">
        update my_pay_order
        <trim prefix="SET" suffixOverrides=",">
            <if test="productId != null and productId != ''">
                product_id = #{productId},
            </if>
            <if test="tradeNo != null">
                trade_no = #{tradeNo},
