            </if>
            <if test="businessCode != null">
                business_code = #{businessCode},
            </if>
        </trim>
        where order_id = #{orderId}
    </update>

    <delete id="deleteMyPayOrderById" parameterType="Long">
        delete
        from my_pay_order
        where id = #{id}
    </delete>

    <delete id="deleteMyPayOrderByIds" parameterType="String">
        delete
        from my_pay_order where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <select id="selectNtPayOrderByOrderIdAndPayWay" resultMap="MyPayOrderResult">
        <include refid="selectMyPayOrderVo"/>
        where order_id = #{orderId}
          and pay_way = #{payWay}
    </select>
